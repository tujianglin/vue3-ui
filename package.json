{
  "name": "tjl-ui",
  "version": "0.1.2",
  "main": "./dist/ui.umd.js",
  "module": "./dist/ui.es.d.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/ui.es.js",
      "require": "./dist/ui.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "scripts": {
    "bootstrap": "pnpm install",
    "dev": "vite",
    "docs:dev": "vitepress dev docs",
    "docs:build": "rimraf docs/guide/changelog.md && pnpm log && vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "build": "pnpm clean:dist && pnpm gen:components && vue-tsc --emitDeclarationOnly && vite build && pnpm gen:typings && pnpm gen:type && pnpm clean:typings",
    "preview": "vite preview",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean:lib": "rimraf node_modules",
    "clean:dist": "rimraf dist && pnpm clean:typings",
    "clean:typings": "rimraf typings",
    "reinstall": "rimraf pnpm-lock.yaml && rimraf package.lock.json && rimraf node_modules && npm run bootstrap",
    "prepare": "husky install",
    "lint:lint-staged": "lint-staged",
    "log": "conventional-changelog -p angular -i docs/guide/changelog.md -s",
    "gen:components": "node src/build/gen-components.js",
    "gen:type": "node src/build/gen-type.js",
    "gen:typings": "node src/build/gen-typings.js"
  },
  "dependencies": {
    "ant-design-vue": "^3.2.5",
    "tjl-ui": "0.1.2",
    "vite-plugin-vitepress-demo": "^1.1.5",
    "vitepress": "^1.0.0-alpha.1",
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.40",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "consola": "^2.15.3",
    "conventional-changelog-cli": "^2.2.2",
    "cz-conventional-changelog-zh": "^0.0.2",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.1.0",
    "fast-glob": "^3.2.11",
    "husky": "^8.0.1",
    "less": "^4.1.2",
    "lint-staged": "^13.0.0",
    "lodash-es": "^4.17.21",
    "postcss": "^8.4.14",
    "postcss-html": "^1.4.1",
    "postcss-less": "^6.0.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "rollup-plugin-delete": "^2.0.0",
    "stylelint": "^14.8.5",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.5.4",
    "vite": "^2.9.9",
    "vite-plugin-dts": "^1.2.0",
    "vite-plugin-style-import": "^2.0.0",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vue-eslint-parser": "^9.0.2",
    "vue-tsc": "^0.34.7"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-zh"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write",
      "stylelint --fix"
    ],
    "*.{scss,less,styl,html}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "files": [
    "dist"
  ]
}
